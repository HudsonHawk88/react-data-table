{
    "name": "@inftechsol/react-data-table",
    "version": "0.6.952",
    "author": {
        "name": "Gergő Tóth",
        "email": "info@inftechsol.hu",
        "url": "https://inftechsol.hu"
    },
    "main": "dist/cjs/index.js",
    "module": "dist/esm/index.js",
    "files": [
        "dist"
    ],
    "repository": {
        "type": "git",
        "url": "https://github.com/HudsonHawk88/react-data-table"
    },
    "private": false,
    "dependencies": {
        "@types/jest": "27.5.1",
        "@types/node": "17.0.32",
        "bootstrap": "5.1.3",
        "react": "18.1.0",
        "react-dom": "18.1.0",
        "react-scripts": "5.0.1",
        "reactstrap": "9.0.2",
        "typescript": "^4.6.4"
    },
    "devDependencies": {
        "@babel/cli": "7.17.10",
        "@types/bootstrap": "5.1.11",
        "@types/prop-types": "15.7.5",
        "@types/react": "18.0.9",
        "@types/react-dom": "18.0.3"
    },
    "scripts": {
        "start": "react-scripts start",
        "copycss": "mkdir dist/cjs/styles && mkdir dist/esm/styles && cp src/styles/index.css dist/cjs/styles/ && cp src/styles/index.css dist/esm/styles/",
        "build": "rm -rf dist/ && prettier --write src/ && npm run build:esm && npm run build:cjs && npm run copycss",
        "build:esm": "tsc",
        "build:cjs": "tsc --module CommonJS --outDir dist/cjs",
        "pub": "npm publish --access public",
        "test": "react-scripts test",
        "eject": "react-scripts eject"
    },
    "eslintConfig": {
        "extends": [
            "react-app",
            "react-app/jest"
        ]
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    }
}
